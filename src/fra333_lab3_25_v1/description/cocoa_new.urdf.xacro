<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro"  name="robot">

  <xacro:property name="link_0_c_offset_x" value="0.0"/>
  <xacro:property name="link_0_c_offset_y" value="0.0"/>
  <xacro:property name="link_0_c_offset_z" value="0.009962"/>

  <xacro:property name="link_1_c_offset_x" value="-0.027383"/>
  <xacro:property name="link_1_c_offset_y" value="0.002656"/>
  <xacro:property name="link_1_c_offset_z" value="-0.067605"/>

  <xacro:property name="link_2_c_offset_x" value="0.099891"/>
  <xacro:property name="link_2_c_offset_y" value="0.010935"/>
  <xacro:property name="link_2_c_offset_z" value="-0.003837"/>

  <xacro:property name="link_3_c_offset_x" value="0.054699"/>
  <xacro:property name="link_3_c_offset_y" value="-0.077878"/>
  <xacro:property name="link_3_c_offset_z" value="0.000125"/>

  <gazebo reference="link_0">
    <mu1>0.2</mu1>
    <mu2>0.2</mu2>
    <material>Gazebo/Red</material>
  </gazebo>

  <gazebo reference="link_1">
    <mu1>0.2</mu1>
    <mu2>0.2</mu2>
    <material>Gazebo/Green</material>
  </gazebo>

  <gazebo reference="link_2">
    <mu1>0.2</mu1>
    <mu2>0.2</mu2>
    <material>Gazebo/Black</material>
  </gazebo>

  <gazebo reference="link_3">
    <mu1>0.2</mu1>
    <mu2>0.2</mu2>
    <material>Gazebo/Blue</material>
  </gazebo>

  <!-- This first link called "world" is empty -->
  <link name="world"></link>

  <!-- A simple fixed joint from our empty world link, to our base. -->
  <!-- The base origin is offset from the world origin. -->
  <joint name="world_joint" type="fixed">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <parent link="world"/>
      <child link="link_0"/>        
  </joint>

  <link name="link_0">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file:///$(find fra333_lab3_25_v1)/meshes/link_0.stl"/>
        <!-- <cylinder radius="0.21" length="0.020"/> -->
      </geometry>
      <material name="Grey"/>
    </visual>

    <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <contact_coefficients mu="0" kp="1000.0" kd="1.0"/>
        <geometry>
            <mesh filename="file:///$(find fra333_lab3_25_v1)/meshes/link_0.stl"/>
            <!-- <cylinder radius="0.21" length="0.020"/> -->
        </geometry>
    </collision>

    <inertial>
      <origin xyz="${link_0_c_offset_x} ${link_0_c_offset_y} ${link_0_c_offset_z}" rpy="0 0 0"/>
      <mass value="0.689781" />
      <inertia ixx="0.001916" ixy="0.000000" ixz="0.000000" iyy="0.001916" iyz="0.000000" izz="0.003787" />
    </inertial>
  </link>

  <joint name="joint_rev_0_1" type="revolute">
    <parent link="link_0"/>
    <child link="link_1"/>
    <origin rpy="0 0 0" xyz="0 0 0.136"/>
    <axis xyz="0 0 1"/>
    <!-- <limit lower="-3.14" upper="3.14" effort="0.5" velocity="0.5"/> -->
    <limit lower="-3.14" upper="3.14" effort="1000.0" velocity="1000.0"/>
  </joint>

  <link name="link_1">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file:///$(find fra333_lab3_25_v1)/meshes/link_1.stl"/>
      </geometry>
      <material name="Orange"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <contact_coefficients mu="0" kp="1000.0" kd="1.0"/>
      <geometry>
          <mesh filename="file:///$(find fra333_lab3_25_v1)/meshes/link_1.stl"/>
          <!-- <box size="2.5 1.5 0.1" /> -->
      </geometry>
    </collision>

    <inertial>
      <origin xyz="${link_1_c_offset_x} ${link_1_c_offset_y} ${link_1_c_offset_z}" rpy="0 0 0"/>
      <mass value="3.584649" />
      <inertia ixx="0.012069" ixy="0.000340" ixz="0.000161" iyy="0.018044" iyz="0.000626" izz="0.024030" />
    </inertial>
  </link>

  <joint name="joint_rev_1_2" type="revolute">
    <parent link="link_1"/>
    <child link="link_2"/>
    <origin rpy="1.5708 -1.5708 0" xyz="0 0 0"/>
    <axis xyz="0 0 1"/>
    <!-- <limit lower="-0.523" upper="0.523" effort="0.5" velocity="0.5"/> -->
    <limit lower="-3.14" upper="3.14" effort="1000.0" velocity="1000.0"/>
  </joint>

  <link name="link_2">
    <visual>
      <origin xyz="0 0 0.0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file:///$(find fra333_lab3_25_v1)/meshes/link_2.stl"/>
      </geometry>
      <material name="Orange"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <contact_coefficients mu="0" kp="1000.0" kd="1.0"/>
      <geometry>
          <mesh filename="file:///$(find fra333_lab3_25_v1)/meshes/link_2.stl"/>
          <!-- <box size="2.5 1.5 0.1" /> -->
      </geometry>
    </collision>

    <inertial>
      <origin xyz="${link_2_c_offset_x} ${link_2_c_offset_y} ${link_2_c_offset_z}" rpy="0 0 0"/>
      <mass value="1.674836" />
      <inertia ixx="0.002594" ixy="-0.000469" ixz="0.000898" iyy="0.012791" iyz="0.000258" izz="0.012890" />
    </inertial>
  </link>

  <joint name="joint_rev_2_3" type="revolute">
    <parent link="link_2"/>
    <child link="link_3"/>
    <origin rpy="0 0 0" xyz="0.26179 0 0"/>
    <axis xyz="0 0 1"/>
    <!-- <limit lower="-1.0472" upper="1.5708" effort="0.5" velocity="0.5"/> -->
    <limit lower="-3.14" upper="3.14" effort="1000.0" velocity="1000.0"/>
  </joint>

  <link name="link_3">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file:///$(find fra333_lab3_25_v1)/meshes/link_3.stl"/>
      </geometry>
      <material name="Blue"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <contact_coefficients mu="0" kp="1000.0" kd="1.0"/>
      <geometry>
          <mesh filename="file:///$(find fra333_lab3_25_v1)/meshes/link_3.stl"/>
          <!-- <box size="2.5 1.5 0.1" /> -->
      </geometry>
    </collision>

    <inertial>
      <origin xyz="${link_3_c_offset_x} ${link_3_c_offset_y} ${link_3_c_offset_z}" rpy="0 0 0"/>
      <mass value="1.783530" />
      <inertia ixx="0.010684" ixy="0.000433" ixz="-0.000003" iyy="0.002620" iyz="-0.000004" izz="0.011479" />
    </inertial>
  </link>

  <ros2_control name="GazeboSystem" type="system">
    <hardware>
      <plugin>gazebo_ros2_control/GazeboSystem</plugin>
    </hardware>

    <joint name="joint_rev_0_1">
        <command_interface name="position">
            <param name="min">-3.14</param>
            <param name="max">3.14</param>
        </command_interface>
        <command_interface name="velocity">
          <param name="min">-10</param>
          <param name="max">10</param>
        </command_interface>
        <command_interface name="effort">
          <param name="min">-1000</param>
          <param name="max">1000</param>
        </command_interface>
        <state_interface name="position">
            <param name="initial_value">0.0</param>
        </state_interface>
        <state_interface name="velocity"/>
        <state_interface name="effort"/>
    </joint>

    <joint name="joint_rev_1_2">
        <command_interface name="position">
            <param name="min">-3.14</param>
            <param name="max">3.14</param>
        </command_interface>
        <command_interface name="velocity">
          <param name="min">-10</param>
          <param name="max">10</param>
        </command_interface>
        <command_interface name="effort">
          <param name="min">-1000</param>
          <param name="max">1000</param>
        </command_interface>
        <state_interface name="position">
            <param name="initial_value">0.0</param>
        </state_interface>
        <state_interface name="velocity"/>
        <state_interface name="effort"/>
    </joint>
    
    <joint name="joint_rev_2_3">
        <command_interface name="position">
            <param name="min">-3.14</param>
            <param name="max">3.14</param>
        </command_interface>
        <command_interface name="velocity">
          <param name="min">-10</param>
          <param name="max">10</param>
        </command_interface>
        <command_interface name="effort">
          <param name="min">-1000</param>
          <param name="max">1000</param>
        </command_interface>
        <state_interface name="position">
            <param name="initial_value">0.0</param>
        </state_interface>
        <state_interface name="velocity"/>
        <state_interface name="effort"/>
    </joint>
  </ros2_control>

  <gazebo>
    <plugin filename="libgazebo_ros2_control.so" name="gazebo_ros2_control">
        <robot_sim_type>gazebo_ros2_control/GazeboSystem</robot_sim_type>
      <parameters>$(find fra333_lab3_25_v1)/config/cocoa_controllers.yaml</parameters>
    </plugin>
</gazebo>

</robot>
